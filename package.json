{
  "name": "vue-dj-gantt",
  "version": "2.4.0",
  "description": "Simple Gantt diagram component for Vue.",
  "main": "dist/VueDjGantt.ssr.js",
  "browser": "dist/VueDjGantt.esm.js",
  "module": "dist/VueDjGantt.esm.js",
  "unpkg": "dist/VueDjGantt.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "scripts": {
    "dev": "vue-cli-service serve dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "dependencies": {
    "moment": "^2.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/storybook-deployer": "^2.8.6",
    "@storybook/vue": "^6.0.21",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-service": "^5.0.8",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "minimist": "^1.2.5",
    "node-sass": "^4.14.1",
    "react-is": "^16.13.1",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-scss": "^2.6.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass-loader": "^10.0.1",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdoleczek/vue-dj-gantt.git"
  },
  "keywords": [
    "gantt",
    "gantt js",
    "gantt chart",
    "javascript gantt",
    "javascript gantt chart",
    "vue gantt",
    "js gantt",
    "gantt-chart",
    "responsive",
    "vue",
    "vue-gantt",
    "project gantt",
    "responsive gantt",
    "gantt component",
    "vue gantt component",
    "vue dj"
  ],
  "author": "Jan Doleczek <jan@doleczek.pl> (https://doleczek.pl)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jdoleczek/vue-dj-gantt/issues"
  }
}
